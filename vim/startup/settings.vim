colorscheme desert256

set number
set expandtab
set tabstop=4
set shiftwidth=4

set list
set listchars=tab:>-,trail:-

set noswapfile

set hlsearch
set incsearch

" Use the system clipboard for yank and put
set clipboard=unnamedplus

" Fix background color
set t_ut=

" Set to 256 color terminal
set t_Co=256
